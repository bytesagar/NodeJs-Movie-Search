<%- include ('partials/header') %> 

<div class="wrapper">
<div class="container">
    <div class="row justify-content-center mt-4 mb-3">
<% data['Search'].forEach((items) => { %>  
        <div class="card m-2" style="width: 18rem;">
            <img src="<%= items.Poster %>" class="card-img-top img-fluid" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%= items.Title %> </h5>
              <p class="card-text text-left">Year: <%= items.Year %> </p> 
              <a href="https://www.imdb.com/title/<%= items.imdbID%>">IMDB Profile</a> 
            </div>
        </div>   
<% }) %> 
</div>
    </div>
</div>


<%- include ('partials/footer') %> 